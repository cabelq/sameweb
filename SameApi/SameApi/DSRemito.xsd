                <SelectCommand>
                  <DbCommand CommandType="Text" ModifiedByUser="true">
                    <CommandText>SELECT  [ID_Pase]
      ,[Nro_Remito]
      ,[Prefijo]
      ,[Numero]
      ,[Anio]
      ,[Extension]
      ,[Fecha_Pase]
