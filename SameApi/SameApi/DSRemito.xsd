  FROM [ml].[SA_V_Pases_por_Expediente] E
  inner join [ml].[RESPONSABLES_DEPENDENCIAS] D ON (D.dep_codigo = E.ID_Origen )
  inner join [ml].PE_Personal P ON (D.per_legajo = P.per_legajo )
